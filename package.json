{
  "name": "hfex-icon",
  "version": "1.1.1",
  "description": "hfex-icon图标库",
  "main": "./src/index.js",
  "author": "UzumakiHan",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json",
    "./iconify-json/hfex-icon.json": "./iconify-json/hfex-icon.json"
  },
  "types": "./typings/typing.d.ts",
  "scripts": {
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0 -n ./changelog-option.js",
    "copy": "copyfiles .github/** src/*js src/vue/*vue icons/* iconify-json/* typings/* .gitignore .npmignore *.js *.tpl package.json *.md .versionrc dist",
    "build": "npm run copy",
    "gitname": "git config --global user.name 'UzumakiHan'",
    "gitemail": "git config --global user.email '1750263786@qq.com'",
    "release": "npm run gitname && npm run gitemail && standard-version",
    "gitpush": "git push --follow-tags origin master"
  },
  "files": [
    "dist",
    "icons",
    "typings",
    "src",
    "iconify-json"
  ],
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@iconify/tools": "^2.2.6",
    "@xmldom/xmldom": "^0.8.6",
    "axios": "^1.3.4",
    "commander": "^10.0.0",
    "commitizen": "^4.3.0",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "ejs": "^3.1.8",
    "lodash": "^4.17.21",
    "standard-version": "^9.5.0",
    "svgpath": "^2.6.0",
    "unplugin-icons": "^0.15.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
